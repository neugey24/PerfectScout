          </div>
          <div class="accordion-body">
            <div class="accordion-content">

              <div class="field">
                <label>Batter Filter Groups:</label>
                <div class="control">
                  <div class="select is-primary is-small">
                    <select id="batter_bat_rat_group" onchange="toggleRatingGroup(this, 'bat');">
                      <option value="contact">Contact Ratings</option>
                      <option value="gap">Gap Ratings</option>
                      <option value="power">Power Ratings</option>
                      <option value="eye">Eye Ratings</option>
                      <option value="avoid-k">Avoid K Ratings</option>
                      <option value="runner">Runner Ratings</option>
                      <option value="bunter">Bunter Ratings</option>
                      <option value="tend">Hitter Tendencies</option>
                    </select>
                  </div>
                </div>
              </div>

              <!-- Batter Contact Ratings group -->
              <section id="batter_bat_rat_group_contact">
              <div class="columns" >
                <div class="column is-three-quarters">

                  <div class="field">
