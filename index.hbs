                  </div>

                  <div class="field">
                    <div class="control">
                      <label for="batter_contact_vs_l_min">Contact (VS L) Rating (minimum):</label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span class="ps-slider-label" id="batter_contact_vs_l_min_output">64</span>
                      <br />0 <input oninput="rangeChange(this);" class="ps-slider" id="batter_contact_vs_l_min" step="1" min="0" max="128" value="64" type="range" /> 128
                    </div>
                  </div>


                </div>
                <div class="column is-one-quarter">

                  <div class="field"><br />
                    <input id="batter_contact_min_on" type="checkbox" name="switchExample" class="switch">
                    <label for="batter_contact_min_on">Use</label>
                  </div>
                  <div class="field"><br />
                    <input id="batter_contact_vs_r_min_on" type="checkbox" name="switchExample" class="switch">
                    <label for="batter_contact_vs_r_min_on">Use</label>
                  </div>
                  <div class="field"><br />
                    <input id="batter_contact_vs_l_min_on" type="checkbox" name="switchExample" class="switch">
                    <label for="batter_contact_vs_l_min_on">Use</label>
                  </div>

                </div>
              </div>
            </section>

              <!-- Batter Gap Ratings group -->
              <section id="batter_bat_rat_group_gap" style="display:none;">
              <div class="columns">
                <div class="column is-three-quarters">

                  <div class="field">
                    <div class="control">
                      <label for="batter_gap_min">Gap (main) Rating (minimum):</label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span class="ps-slider-label" id="batter_gap_min_output">64</span>
                      <br />0 <input oninput="rangeChange(this);" class="ps-slider" id="batter_gap_min" step="1" min="0" max="128" value="64" type="range" /> 128
